{
  "name": "Internet Archive Enhancer",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Enhance archive.org with inline metadata editing and a black and white UI.",
  "content_scripts": [
    {
      "matches": ["https://archive.org/details/*"],
      "css": ["ia_enhancer.css"],
      "js": ["jquery-2.0.2.js", "ia_enhancer.js"]
    }
  ],
  "web_accessible_resources": [
    "logo.png"
  ],
  "icons": {
    "128": "logo_128.png"
  }
}
